{
  "name": "client",
  "version": "0.0.1",
  "description": "a collection of smart agents",
  "productName": "MAS hub",
  "author": "geniusfaker <geniusfaker@gmail.com>",
  "private": true,

  "scripts": {
    "postinstall": "cd ../backend/ && npm install",
    "preserve": "warp project select",
    "serve:client": "quasar serve",
    "serve:server": "warp dev ../backend/",
    "serve": "run-p serve:*",
    "build:client": "quasar build",
    "build:server": "warp build ../backend/",
    "build": "run-s build:server build:client",
    "deploy": "warp deploy ./ ../backend/",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "pinia": "^2.0.11",
    "@quasar/extras": "^1.0.0",
    "core-js": "^3.6.5",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "@warpjs/engine": "^4.0.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.19.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.5.1",
    "@types/node": "^12.20.21",
    "@quasar/app-webpack": "^3.0.0",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.19",
    "@vue/cli-service": "~4.5.13",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-import": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.14",
    "warp": "^4.0.13"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
